function _npm_access {
    local line state

    function _commands {
        local -a commands
        commands=(
            'public:Set a package to be publicly accessible'
            'restricted:Set a package to be publicly restricted'
            'grant:Add the ability of users and teams to have read-only or read-write access to a package.'
            'revoke:Remove the ability of users and teams to have read-only or read-write access to a package.'
            '2fa-required:Configure a package to require that anyone publishing it have two-factor authentication enabled on their account.'
            '2fa-not-required:Configure a package NOT to require two-factor authentication.'
            'ls-packages:Show all  of the packages a user or a team is able to access, along with the access level, except for read-only public packages'
            'ls-collaborators:Show all of the access privileges for a package. Will only show permissions for packages to which you have at least read access.'
            'edit:Set the access privileges for a package at once using $EDITOR.'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (public)
            _npm_access_public
            ;;

        (restricted)
            _npm_access_restricted
            ;;

        (grant)
            _npm_access_grant
            ;;

        (revoke)
            _npm_access_revoke
            ;;

        (2fa-required)
            _npm_access_2fa-required
            ;;

        (2fa-not-required)
            _npm_access_2fa-not-required
            ;;

        (ls-packages)
            _npm_access_ls-packages
            ;;

        (ls-collaborators)
            _npm_access_ls-collaborators
            ;;

        (edit)
            _npm_access_edit
            ;;

        esac
        ;;
     esac

}

