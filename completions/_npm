function _npm_team {
    local line state

    function _commands {
        local -a commands
        commands=(
            'create:Create a new team.'
            'destroy:Destroy a team.'
            'add:Add a user to an existing team.'
            'rm:Using npm team rm you can also remove users from a team they belong to.'
            'ls:If performed on an organization name, will return a list of existing teams under that organization.'
        )
        _describe 'command' commands
    }
 

    _arguments -C \
        ': :->cmd' \
        '*:: :->subcmd'

    case $state in
    (cmd)
        _commands
        ;;
    (subcmd)
        case $line[1] in
        (create)
            _npm_team_create
            ;;

        (destroy)
            _npm_team_destroy
            ;;

        (add)
            _npm_team_add
            ;;

        (rm)
            _npm_team_rm
            ;;

        (ls)
            _npm_team_ls
            ;;

        esac
        ;;
     esac

}

