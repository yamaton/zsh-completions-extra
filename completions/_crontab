#compdef crontab

# Auto-generated with h2o

function _crontab {
    _arguments \
        '-u[specify user name]' \
        '-e[edit user'\''s crontab]' \
        '-l[list user'\''s crontab]' \
        '-r[delete user'\''s crontab]' \
        '-i[prompt before deleting user'\''s crontab]' \
        "*: :_files"

}

_crontab "$@"

