#compdef iconv

# Auto-generated with h2o

function _iconv {
    _arguments \
        {-f,--from-code}'[the encoding of the input]' \
        {-t,--to-code}'[the encoding of the output]' \
        '-c[discard unconvertible characters]' \
        '--unicode-subst[substitution for unconvertible Unicode characters]' \
        '--byte-subst[substitution for unconvertible bytes]' \
        '--widechar-subst[substitution for unconvertible wide characters]' \
        {-s,--silent}'[suppress error messages about conversion problems]' \
        {-l,--list}'[list the supported encodings]' \
        '--help[display this help and exit]' \
        '--version[output version information and exit]' \
        "*: :_files"

}

_iconv "$@"

