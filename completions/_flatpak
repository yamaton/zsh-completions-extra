}

function _flatpak_build-sign {
    _arguments \
        {-h,--help}'[Show help options]' \
        '--arch[Arch to install for]' \
        '--runtime[Look for runtime with the specified name]' \
        '--gpg-sign[GPG Key ID to sign the commit with]' \
