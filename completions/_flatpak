function _flatpak_build-init {
    _arguments \
        {-h,--help}'[Show help options]' \
        '--arch[Arch to use]' \
        {-v,--var}'[Initialize var from named runtime]' \
        '--base[Initialize apps from named app]' \
        '--base-version[Specify version for --base]' \
        '--base-extension[Include this base extension]' \
        '--extension-tag[Extension tag to use if building extension]' \
        {-w,--writable-sdk}'[Initialize /usr with a writable copy of the sdk]' \
        '--type[Specify the build type (app, runtime, extension)]' \
        '--tag[Add a tag]' \
        '--sdk-extension[Include this sdk extension in /usr]' \
        '--extension[Add extension point info]' \
        '--sdk-dir[Where to store sdk (defaults to '\''usr'\'')]':file:_files \
        '--update[Re-initialize the sdk/var]' \
        '--verbose[Show debug information, -vv for more detail]' \
        '--ostree-verbose[Show OSTree debug information]' \
        "*: :_files"

}

function _flatpak_build {
    _arguments \
        {-h,--help}'[Show help options]' \
        '--help-all[Show all help options]' \
        {-r,--runtime}'[Use Platform runtime rather than Sdk]' \
        '--readonly[Make destination readonly]' \
        '--bind-mount[Add bind mount]' \
        '--build-dir[Start build in this directory]':file:_files \
        '--sdk-dir[Where to look for custom sdk dir (defaults to '\''usr'\'')]':file:_files \
        '--metadata[Use alternative file for the metadata]':file:_files \
        {-p,--die-with-parent}'[Kill processes when the parent process dies]' \
        '--with-appdir[Export application homedir directory to build]' \
        '--log-session-bus[Log session bus calls]' \
        '--log-system-bus[Log system bus calls]' \
        {-v,--verbose}'[Show debug information, -vv for more detail]' \
        '--ostree-verbose[Show OSTree debug information]' \
        '--share[Share with host]' \
        '--unshare[Unshare with host]' \
        '--socket[Expose socket to app]' \
        '--nosocket[Don'\''t expose socket to app]' \
        '--device[Expose device to app]' \
        '--nodevice[Don'\''t expose device to app]' \
        '--allow[Allow feature]' \
        '--disallow[Don'\''t allow feature]' \
        '--filesystem[Expose filesystem to app (:ro for read-only)]':file:_files \
        '--nofilesystem[Don'\''t expose filesystem to app]':file:_files \
        '--env[Set environment variable]' \
        '--env-fd[Read environment variables in env -0 format from FD]' \
        '--unset-env[Remove variable from environment]' \
        '--own-name[Allow app to own name on the session bus]' \
        '--talk-name[Allow app to talk to name on the session bus]' \
        '--no-talk-name[Don'\''t allow app to talk to name on the session bus]' \
        '--system-own-name[Allow app to own name on the system bus]' \
        '--system-talk-name[Allow app to talk to name on the system bus]' \
        '--system-no-talk-name[Don'\''t allow app to talk to name on the system bus]' \
        '--add-policy[Add generic policy option]' \
        '--remove-policy[Remove generic policy option]' \
        '--persist[Persist home directory subpath]':file:_files \
        "*: :_files"

}

function _flatpak_build-finish {
    _arguments \
        {-h,--help}'[Show help options]' \
        '--help-all[Show all help options]' \
        '--command[Command to set]' \
        '--require-version[Flatpak version to require]' \
        '--no-exports[Don'\''t process exports]' \
        '--extra-data[Extra data info]' \
        '--extension[Add extension point info]' \
        '--remove-extension[Remove extension point info]' \
        '--extension-priority[Set extension priority (only for extensions)]' \
        '--sdk[Change the sdk used for the app]' \
        '--runtime[Change the runtime used for the app]' \
        '--metadata[Set generic metadata option]' \
        '--no-inherit-permissions[Don'\''t inherit permissions from runtime]' \
        {-v,--verbose}'[Show debug information, -vv for more detail]' \
        '--ostree-verbose[Show OSTree debug information]' \
        '--share[Share with host]' \
        '--unshare[Unshare with host]' \
        '--socket[Expose socket to app]' \
        '--nosocket[Don'\''t expose socket to app]' \
        '--device[Expose device to app]' \
        '--nodevice[Don'\''t expose device to app]' \
        '--allow[Allow feature]' \
        '--disallow[Don'\''t allow feature]' \
        '--filesystem[Expose filesystem to app (:ro for read-only)]':file:_files \
        '--nofilesystem[Don'\''t expose filesystem to app]':file:_files \
        '--env[Set environment variable]' \
        '--env-fd[Read environment variables in env -0 format from FD]' \
        '--unset-env[Remove variable from environment]' \
        '--own-name[Allow app to own name on the session bus]' \
        '--talk-name[Allow app to talk to name on the session bus]' \
        '--no-talk-name[Don'\''t allow app to talk to name on the session bus]' \
        '--system-own-name[Allow app to own name on the system bus]' \
        '--system-talk-name[Allow app to talk to name on the system bus]' \
        '--system-no-talk-name[Don'\''t allow app to talk to name on the system bus]' \
        '--add-policy[Add generic policy option]' \
        '--remove-policy[Remove generic policy option]' \
        '--persist[Persist home directory subpath]':file:_files \
        "*: :_files"

}

function _flatpak_build-export {
    _arguments \
        {-h,--help}'[Show help options]' \
        {-s,--subject}'[One line subject]' \
        {-b,--body}'[Full description]' \
        '--arch[Architecture to export for (must be host compatible)]' \
        {-r,--runtime}'[Commit runtime (/usr), not /app]' \
        '--update-appstream[Update the appstream branch]' \
        '--no-update-summary[Don'\''t update the summary]' \
        '--files[Use alternative directory for the files]':file:_files \
        '--metadata[Use alternative file for the metadata]':file:_files \
        '--gpg-sign[GPG Key ID to sign the commit with]' \
        '--exclude[Files to exclude]' \
        '--include[Excluded files to include]' \
        '--gpg-homedir[GPG Homedir to use when looking for keyrings]':file:_files \
        '--subset[Add to a named subset]' \
        '--end-of-life[Mark build as end-of-life]' \
        '--end-of-life-rebase[Mark build as end-of-life, to be replaced with the given ID]' \
        '--token-type[Set type of token needed to install this commit]' \
        '--timestamp[Override the timestamp of the commit]' \
        '--collection-id[Collection ID]' \
        '--disable-fsync[Do not invoke fsync()]' \
        '--disable-sandbox[Do not sandbox icon validator]' \
        '--no-summary-index[Don'\''t generate a summary index]' \
        {-v,--verbose}'[Show debug information, -vv for more detail]' \
        '--ostree-verbose[Show OSTree debug information]' \
        "*: :_files"

}

function _flatpak_build-bundle {
    _arguments \
        {-h,--help}'[Show help options]' \
        '--runtime[Export runtime instead of app]' \
        '--arch[Arch to bundle for]' \
        '--repo-url[Url for repo]' \
        '--runtime-repo[Url for runtime flatpakrepo file]' \
        '--gpg-keys[Add GPG key from FILE (- for stdin)]':file:_files \
        '--gpg-sign[GPG Key ID to sign the OCI image with]' \
        '--gpg-homedir[GPG Homedir to use when looking for keyrings]':file:_files \
        '--from-commit[OSTree commit to create a delta bundle from]' \
        '--oci[Export oci image instead of flatpak bundle]' \
        {-v,--verbose}'[Show debug information, -vv for more detail]' \
        '--ostree-verbose[Show OSTree debug information]' \
        "*: :_files"

}

